{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "RequestResponseLogging": {
    "IsEnabled": true,
    "IncludeRequestHeaders": true,
    "IncludeResponseHeaders": true,
    "IncludeRequestBody": false,
    "IncludeResponseBody": false,
    "MaxBodySizeToLog": 8192
  }
}
