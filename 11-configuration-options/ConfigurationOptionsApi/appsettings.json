{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "RequestResponseLogging": {
    "IsEnabled": true,
    "IncludeRequestHeaders": false,
    "IncludeResponseHeaders": false,
    "IncludeRequestBody": true,
    "IncludeResponseBody": true,
    "MaxBodySizeToLog": 4096
  }
}
